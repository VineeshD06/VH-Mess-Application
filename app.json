{
  "expo": {
    "name": "vh-mess",
    "slug": "vh-mess",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IIT-Kanpur.png",
    "scheme": "vhmess",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.vh-mess"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IIT-Kanpur.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.vhmess"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/IIT-Kanpur.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/IIT-Kanpur.png",
            "backgroundColor": "#0f172a"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
